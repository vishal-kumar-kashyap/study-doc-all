<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action  
Mapping 7.0.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_0_0.dtd">

<resource-action-mapping>

	<model-resource>
		<model-name>com.adjecti.pis.registration</model-name>
		<portlet-ref>
			<portlet-name>com_adjecti_pis_web_RegistrationRequestWebPortlet</portlet-name>
		</portlet-ref>
		<root>true</root>
		<permissions>
			<supports>
				<action-key>UPLOAD_REGISTRATIONS</action-key>
				<action-key>ADD_REGISTRATION</action-key>
				<action-key>VIEW</action-key>
			</supports>
			<site-member-defaults>
			<action-key>UPLOAD_REGISTRATIONS</action-key>
				<action-key>ADD_REGISTRATION</action-key>
			</site-member-defaults>
			<guest-defaults>
				<action-key>VIEW</action-key>
			</guest-defaults>
			<guest-unsupported>
				<action-key>UPLOAD_REGISTRATIONS</action-key>
				<action-key>ADD_REGISTRATION</action-key>
			</guest-unsupported>
		</permissions>
	</model-resource>


 <model-resource>
       <model-name>com.adjecti.pis.model.RegistrationRequest</model-name>
        <portlet-ref>
          <portlet-name>com_adjecti_pis_web_RegistrationRequestWebPortlet</portlet-name>
        </portlet-ref>
      <permissions>
            <supports>
            <action-key>UPLOAD_REGISTRATIONS</action-key>
                <action-key>ADD_REGISTRATION</action-key>
                <action-key>DELETE</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
                <action-key>VIEW</action-key>
            </supports>
            <site-member-defaults>
            <action-key>UPLOAD_REGISTRATIONS</action-key>
                <action-key>ADD_REGISTRATION</action-key>
                <action-key>VIEW</action-key>
            </site-member-defaults>
            <guest-defaults>
                <action-key>VIEW</action-key>
            </guest-defaults>
            <guest-unsupported>
                <action-key>UPDATE</action-key>
            </guest-unsupported>
        </permissions>
    </model-resource>

</resource-action-mapping>